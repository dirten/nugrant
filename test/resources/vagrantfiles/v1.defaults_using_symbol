Vagrant::Config.run do |config|
  config.user.defaults = {
    :single => 1,
    :local => {
        :first => "value1",
        :second => "value2"
    }
  }

  puts config.user.single
  puts config.user[:single]

  puts config.user.local.first
  puts config.user[:local]["first"]

  puts config.user.local.second
  puts config.user["local"][:second]
end
